# Base Sepolia RPC URL (public endpoint)
RPC_URL=https://sepolia.base.org

# Your private key (DO NOT commit this - add .env to .gitignore)
# Get from your wallet (e.g., MetaMask -> Account Details -> Export Private Key)
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# BaseScan API Key (for contract verification)
# Get from: https://basescan.org/myapikey
BASESCAN_API_KEY=your_basescan_api_key_here

# Deployer address (will be populated automatically)
DEPLOYER_ADDRESS=
